{
    "name": "@radoslaw-medryk/react-template",
    "private": true,
    "scripts": {
        "build-dev": "mkdir -p dist && rm -r dist && npm run prettier-check && tsc && webpack --mode=production && cp configs/dev-config.json dist/config.json",
        "build-prod": "mkdir -p dist && rm -r dist && npm run prettier-check && tsc && webpack --mode=production && cp configs/prod-config.json dist/config.json",
        "deploy-prod": "npm run build-prod && npx firebase deploy --project <app_name>",
        "deploy-dev": "npm run build-dev && npx firebase deploy --project <dev_app_name>",
        "start": "webpack-dev-server",
        "lint:css": "stylelint src/**/*.tsx",
        "prettier": "prettier ./src/**/*.{js,ts,tsx,json,html} --write",
        "prettier-check": "prettier ./src/**/*.{js,ts,tsx,json,html} --check"
    },
    "dependencies": {
        "@babel/core": "7.7.4",
        "@babel/plugin-proposal-class-properties": "7.7.4",
        "@babel/polyfill": "7.7.0",
        "@babel/preset-env": "7.7.4",
        "@babel/preset-react": "7.7.4",
        "@babel/preset-typescript": "7.7.4",
        "@radoslaw-medryk/tslint": "0.1.8",
        "@types/classnames": "2.2.9",
        "@types/react": "16.9.13",
        "@types/react-dom": "16.9.4",
        "@types/react-router": "5.1.3",
        "@types/react-router-dom": "5.1.2",
        "axios": "0.21.1",
        "babel-loader": "8.0.6",
        "babel-plugin-module-resolver": "3.2.0",
        "classnames": "2.2.6",
        "css-loader": "3.2.0",
        "file-loader": "5.0.2",
        "html-webpack-plugin": "3.2.0",
        "linaria": "1.3.1",
        "mini-css-extract-plugin": "0.8.0",
        "normalize.css": "8.0.1",
        "react": "16.12.0",
        "react-dom": "16.12.0",
        "react-router": "5.1.2",
        "react-router-dom": "5.1.2",
        "source-map-loader": "0.2.4",
        "stylelint": "12.0.0",
        "stylelint-config-recommended": "3.0.0",
        "stylelint-webpack-plugin": "1.1.0",
        "tslint": "5.20.1",
        "tslint-loader": "3.5.4",
        "typescript": "3.7.2",
        "url-loader": "2.3.0",
        "webpack": "4.41.2",
        "webpack-cli": "3.3.10"
    },
    "devDependencies": {
        "firebase-tools": "7.8.1",
        "prettier": "1.19.1",
        "style-loader": "1.0.0",
        "tslint-config-prettier": "1.18.0",
        "webpack-dev-server": "3.9.0"
    }
}
